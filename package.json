{
  "name": "aldi_bot",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/Vafflee/aldi_bot",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.4",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "scripts": {
    "dev": "nodemon src/index.ts",
    "prepare": "husky install",
    "precommit-hook": "yarn run check-types && lint-staged",
    "check-types": "tsc --noEmit",
    "db": "docker run --name aldi-pg -p 5432:5432 -e POSTGRES_USER=aldibotadmin -e POSTGRES_PASSWORD=aldibotpgpwd -e POSTGRES_DB=aldibotpg -d postgres"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@notionhq/client": "^2.2.5",
    "@prisma/client": "^4.15.0",
    "@telegraf/session": "^2.0.0-beta.6",
    "@vitalets/google-translate-api": "^9.2.0",
    "axios": "^1.4.0",
    "better-sqlite3": "^8.4.0",
    "dotenv": "^16.1.4",
    "eslint": "^8.42.0",
    "jsdom": "^22.1.0",
    "kysely": "^0.25.0",
    "prisma": "^4.15.0",
    "telegraf": "^4.12.3-canary.1"
  }
}
