{
  "name": "aldi_bot",
  "version": "1.0.0",
  "main": "build/index.js",
  "repository": "https://github.com/Vafflee/aldi_bot",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/pg": "^8.10.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "start:prod": "yarn run migrate && yarn start",
    "migrate": "prisma migrate deploy",
    "build": "tsc -p .",
    "dev": "nodemon src/index.ts",
    "db": "docker-compose up",
    "prepare": "husky install",
    "precommit-hook": "yarn run check-types && lint-staged",
    "check-types": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=production eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.15.0",
    "@telegraf/session": "^2.0.0-beta.6",
    "axios": "^1.4.0",
    "dotenv": "^16.1.4",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.42.0",
    "kysely": "^0.25.0",
    "pg": "^8.11.0",
    "prisma": "^4.15.0",
    "telegraf": "^4.12.3-canary.1"
  }
}
